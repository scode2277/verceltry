{
  "buildCommand": "bash vercel_build.sh",
  "outputDirectory": "docs/dist",
  "redirects": [
    {
      "source": "/book",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/:section",
      "destination": "/:section/overview",
      "permanent": true
    },
    {
      "source": "/:section/",
      "destination": "/:section/overview",
      "permanent": true
    }
  ]
}

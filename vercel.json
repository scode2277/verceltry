{
  "buildCommand": "bash vercel_build.sh",
  "outputDirectory": "docs/dist",
  "rewrites": [
    {
      "source": "/intro",
      "destination": "/intro/introduction"
    },
    {
      "source": "/contribute/",
      "destination": "/contribute/contributing",
    },
    {
      "source": "/intro",
      "destination": "/intro/introduction",
    },
    {
      "source": "/contribute",
      "destination": "/contribute/contributing",
    },
    {
      "source": "/:section",
      "destination": "/:section/overview",
    },
    {
      "source": "/:section/",
      "destination": "/:section/overview",
    }
  ]
}

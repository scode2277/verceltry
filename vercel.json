{
  "buildCommand": "bash vercel_build.sh",
  "outputDirectory": "docs/dist",
  "rewrites": [
    { "source": "", "destination": "/intro/introduction" },
    { "source": "/", "destination": "/intro/introduction" },
    { "source": "/:section/overview", "destination": "/:section/README" },
    { "source": "/intro", "destination": "/intro/introduction" },
    { "source": "/intro/", "destination": "/intro/introduction" },
    { "source": "/contribute", "destination": "/contribute/contributing" },
    { "source": "/contribute/", "destination": "/contribute/contributing" },
    { "source": "/:section", "destination": "/:section/overview" },
    { "source": "/:section/overview", "destination": "/:section/README" },
    { "source": "/:section/", "destination": "/:section/overview" }
  ]
}
